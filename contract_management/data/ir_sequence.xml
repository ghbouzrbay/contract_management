<odoo>
    <data noupdate="1">

        <!-- Séquence pour les commandes -->
        <record id="seq_custom_order" model="ir.sequence">
            <field name="name">Commande Personnalisée</field>
            <field name="code">custom.order</field>
            <field name="prefix">CMD/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
        </record>

	
	
	<!-- Séquence por les livraisons -->
	<record id="seq_custom_delivery" model="ir.sequence">
    	   <field name="name">Livraison</field>
           <field name="code">custom.delivery</field>
           <field name="prefix">DLV-</field>
           <field name="padding">4</field>
           <field name="company_id" eval="False"/>
	</record>


    </data>
</odoo>

